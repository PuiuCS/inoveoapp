<?xml version='1.0' encoding='utf-8'?>
<widget id="ro.inoveo.inoveoapp" version="1.0.0.alpha05" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Inoveo</name>
